<div class="content l-content">
	<h1 class="content__header">100 Freestyle</h1>
	<form class="entryform" method="post" action="/event/times">
	
	<input type="hidden" name="event" value="100 Freestyle">

		<ol class="competitors__list">
			
			<% i = 0
			while i < @peoplein100FR.length %>


			<li class="competitors__listitem competitors__listitem--even">
				<p class="competitors__name"><%= @peoplein100FR[i]  %></p><p class="competitors__name"><%= @colleges100FR[i] %> | <%= @confs100FR[i][0] %></p>
				<div class="competitors__time">
					<input class="competitors__textfield" type="text" name="<%= @iDs100FR[i]%>-time" placeholder="time"> 
					<p>sec.</p>
				</div>
			</li>

			<% i+=1
			end %>



		</ol>
		<input class="entryform__submit l-right" type="submit" name="Submit">
	</form>
</div>